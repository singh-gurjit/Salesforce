{
    "SLDS button brand": {
        "prefix": "btnbrand",
        "body": [
            ".brand-button {",
            "\t--slds-c-button-brand-color-background: orangered;",
            "\t--slds-c-button-brand-color-background-hover: rgb(186, 50, 0);",
            "\t--slds-c-button-brand-color-border: orangered;",
            "\t--slds-c-button-brand-color-border-hover: rgb(186, 50, 0);",
            "\t--slds-c-button-brand-text-color: #fff;",
            "}"
        ],
        "description": "Brand button color"
    },
    "SLDS utility icon color": {
        "prefix": "icnutl",
        "body": [
            ".utility-icon {",
            "\t--sds-c-icon-color-foreground-default: hsl(213, 93%, 65%);",
            "}"
        ],
        "description": "Utility icon color"
    },
    "SLDS utility icon color background": {
        "prefix": "icnutlbg",
        "body": [
            ".utility-icon {",
            "\t--sds-c-icon-color-foreground: purple;",
            "\t--sds-c-icon-color-background: pink;",
            "}"
        ],
        "description": "Utility icon color background"
    },
    "Field Set with legend": {
        "prefix": "fds",
        "body": [
            "fieldset {",
            "\tdisplay: flex;",
            "\talign-items: center;",
            "\tjustify-content: center;",
            "\tborder-radius: 0.5rem;",
            "\tpadding: 0.25rem 0 0.75rem;",
            "\tborder: 2px solid hsl(213, 94%, 62%);",
            "}",
            "",
            "legend {",
            "\tcolor: hsl(213, 94%, 62%);",
            "\tmargin-left: 1rem;",
            "\tfont-size: 1rem;",
            "\tfont-weight: 600;",
            "}",
            "",
            "section {",
            "\tbackground: #fff;",
            "\tborder-radius: 0.5rem;",
            "\tpadding: 0.5rem;",
            "}"
        ],
        "description": "Field set style"
    },
    "brand color": {
        "prefix": "icnutlbg",
        "body": [
            "color: hsl(213, 94%, 62%);"
        ],
        "description": "brand color"
    }
}